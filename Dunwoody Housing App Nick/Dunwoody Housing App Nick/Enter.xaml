<Window x:Class="Dunwoody_Housing_App_Nick.Enter"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Dunwoody_Housing_App_Nick"
        mc:Ignorable="d"
    
        Title="Enter" Height="450" Width="800">

    <StackPanel>

        <Image Source='Images/DunHead.jpg' Height='181' Margin='-298,0,-300.4,0' />

        <TextBox x:Name="FormUser" Height="23" Width="200" HorizontalAlignment="Center" Margin="20" VerticalAlignment="Top" TextWrapping="Wrap" Text="" TextChanged="Username_TextChanged"/>
        <TextBox x:Name="FormPassword" Height="23" Width="200" HorizontalAlignment="Center" Margin="10" VerticalAlignment="Center" TextWrapping="Wrap"  TextChanged="Password_TextChanged"/>
        <Button Content='Button' Name='auth_button'  Margin='234,0,245.6,0' Click='Auth_button_Click'/>

    </StackPanel>
</Window>

